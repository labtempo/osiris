<AnchorPane id="AnchorPane" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.uff.labtempo.osiris.monitor.controllers.FXMLController">
    <children>
        <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="SensorNet" fx:id ="tabSensorNet">
                    <content>
                        <fx:include fx:id="sensorNet" source="SensorNet.fxml"/>
                    </content>
                </Tab>
                <Tab text="Virtual SensorNet" fx:id ="tabVirtualSensorNet" >
                    <content>
                        <Label alignment="center" maxWidth="Infinity"  maxHeight="Infinity"  VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS" text="Work in progress"/> 
                    </content>
                </Tab>                
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
